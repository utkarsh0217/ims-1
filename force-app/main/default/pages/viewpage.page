<apex:page standardController="Enquiry_Form__c" lightningStylesheets="true">
    <apex:form >
    
    <apex:pageBlock title="enquiry view page">
         
        <apex:pageBlockSection title="information" collapsible="true" columns="2">
            
            <apex:inputField value="{!Enquiry_Form__c.First_Name__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.Last_Name__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.Parent_name__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.College_name__c}"/>
        
        </apex:pageBlockSection>
        
        <apex:pageBlockSection title="educational details" columns="1">
            <apex:inputField value="{!Enquiry_Form__c.Passing_year__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.Qualification__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.Current_status__c}"/>
            <apex:inputField value="{!Enquiry_Form__c.Semester__c}"/>
            
            
        
        </apex:pageBlockSection>
      <apex:pageBlockButtons location="bottom">
          <apex:commandButton action="{!save}" value="save the record"/>
        </apex:pageBlockButtons>
    
    </apex:pageBlock>
        </apex:form>
    
</apex:page>