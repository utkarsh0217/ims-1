<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Must_Be_Between_1_To_5</fullName>
    <active>true</active>
    <errorConditionFormula>Rating__c &lt;1  ||  Rating__c &gt;5</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Rating must be between 1 to 5</errorMessage>
</ValidationRule>
