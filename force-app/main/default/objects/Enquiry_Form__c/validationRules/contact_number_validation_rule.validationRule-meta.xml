<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>contact_number_validation_rule</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( Contact_No__c ) &lt;&gt; 10</errorConditionFormula>
    <errorDisplayField>Contact_No__c</errorDisplayField>
    <errorMessage>contact number should be 10 digit</errorMessage>
</ValidationRule>
