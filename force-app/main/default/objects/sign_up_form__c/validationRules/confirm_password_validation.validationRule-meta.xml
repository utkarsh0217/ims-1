<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>confirm_password_validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(  Conform_password__c   , &quot;((?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[@#$%]).{6,20})&quot;))</errorConditionFormula>
    <errorDisplayField>Conform_password__c</errorDisplayField>
    <errorMessage>it should match with password</errorMessage>
</ValidationRule>
